        <Label alignment="CENTER" layoutX="19.0" layoutY="43.0" style="&#10;" text="PAUSED" textAlignment="CENTER" textFill="#2db2ef" wrapText="false">
          <font>
            <Font name="Roboto Bold" size="37.0" />
          </font>
        </Label>
            <Button fx:id="continueBtn" alignment="CENTER" layoutX="19.0" layoutY="133.0" mnemonicParsing="false" onAction="#continueBtnClicked" prefHeight="37.0" prefWidth="267.0" style="-fx-border-color: #ad88f7;" text="Continue" textFill="#ad88f7">
