<VBox prefHeight="208.0" prefWidth="326.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="208.0" prefWidth="326.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="playBtn" alignment="CENTER" layoutX="127.0" layoutY="170.0" mnemonicParsing="false" text="Play game" />
