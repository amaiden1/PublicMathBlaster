          <font>
            <Font name="Roboto Medium" size="18.0" />
          </font>
        </Label>
            <VBox layoutX="101.0" layoutY="37.0" prefHeight="145.0" prefWidth="158.0">
               <children>
                  <Label text="Choose Your Difficulty" />
                  <RadioButton contentDisplay="CENTER" mnemonicParsing="false" text="Difficulty 1" />
                  <RadioButton mnemonicParsing="false" text="Difficulty 2" />
                  <RadioButton mnemonicParsing="false" text="Difficulty 3" />
                  <RadioButton mnemonicParsing="false" text="Difficulty 4" />
                  <RadioButton mnemonicParsing="false" text="Difficulty 5" />
